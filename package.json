{
  "name": "type-system-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start-prod": "node dist/index.js",
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc -w",
    "test": "jest",
    "prettier": "prettier --config .prettierrc --check '**/*.{ts,js,json}'",
    "prettify": "prettier -l --config .prettierrc --write '**/*.{ts,js,json}'",
    "prettier-watch": "onchange '**/*.{ts,js,json}' -- prettier --write {{changed}}",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blalasaadri/type-system-typescript.git"
  },
