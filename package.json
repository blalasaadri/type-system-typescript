  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blalasaadri/type-system-typescript/issues"
  },
  "homepage": "https://github.com/blalasaadri/type-system-typescript#readme",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "jest": "^24.8.0",
    "onchange": "^6.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  }
}
